# define our source to loook for gems
source "http://rubygems.org/"
# declare the sinatra framework dependency
gem "sinatra" 
gem 'thin'
#using rack to run application
gem 'rack'
#using rake to build run tasks
gem 'rake'

group :test do
	gem "rspec"
end
# require a relative gem version
gem "i18n", "~> 0.4.1"